{
  "name": "geoip-api-vercel",
  "version": "2.0.0",
  "description": "High-performance GeoIP API service optimized for Vercel Serverless Functions",
  "main": "api/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vercel dev",
    "build": "node scripts/build-index.js",
